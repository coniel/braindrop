{
  "name": "minddrop-md",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.28.5",
    "husky": "^8.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.27.1"
}
