{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main", "create-first-workspace"],
  "permissions": [
    "core:webview:allow-create-webview-window",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    "upload:allow-download",
    {
      "identifier": "fs:scope",
      "requireLiteralLeadingDot": false,
      "allow": [
        "$APPDATA",
        "$APPDATA/**",
        "$DOCUMENT",
        "$DOCUMENT/**",
        "$DOCUMENT/**/.minddrop",
        "$DOCUMENT/**/.minddrop/**",
        "$DESKTOP",
        "$DESKTOP/**",
        "$DESKTOP/**/.minddrop",
        "$DESKTOP/**/.minddrop/**",
        "$HOME",
        "$HOME/**",
        "$HOME/**/.minddrop",
        "$HOME/**/.minddrop/**",
        "$APPCONFIG",
        "$APPCONFIG/**"
      ]
    },
    "core:webview:allow-internal-toggle-devtools",
    "core:window:allow-create",
    "core:window:allow-show",
    "core:window:allow-close",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "dialog:allow-open",
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "fs:default",
    "fs:scope-appdata-recursive",
    "fs:scope-applocaldata-recursive",
    "fs:scope-appconfig-recursive",
    "fs:scope-app-recursive",
    "fs:allow-app-read-recursive",
    "fs:allow-app-write-recursive",
    "fs:allow-app-meta-recursive",
    "dialog:default",
    "shell:default",
    "os:default"
  ]
}
