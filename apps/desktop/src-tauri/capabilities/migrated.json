{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    "upload:allow-download",
    {
      "identifier": "fs:scope",
      "requireLiteralLeadingDot": false,
      "require_literal_leading_dot": false,
      "allow": [
        "$APPDATA",
        "$APPDATA/**",
        "$DOCUMENT",
        "$DOCUMENT/**",
        "$DOCUMENT/**/.minddrop",
        "$DOCUMENT/**/.minddrop/**",
        "$DESKTOP",
        "$DESKTOP/**",
        "$DESKTOP/**/.minddrop",
        "$DESKTOP/**/.minddrop/**",
        "$HOME",
        "$HOME/**",
        "$HOME/**/.minddrop",
        "$HOME/**/.minddrop/**",
        "$APPCONFIG",
        "$APPCONFIG/**"
      ]
    },
    "window:allow-create",
    "window:allow-show",
    "window:allow-close",
    "window:allow-start-dragging",
    "shell:allow-open",
    "dialog:allow-open",
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "fs:default",
    "dialog:default",
    "shell:default",
    "os:default"
  ]
}
